{
  "security.workspace.trust.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": false,
  "languageStylus.useSeparator": false,
  "editor.fontSize": 16,
  // Fira Code: https://fonts.google.com/specimen/Fira+Code
  // Iosevka: https://typeof.net/Iosevka/
  // Rec Mono Semicasual: https://www.recursive.design
  "editor.fontFamily": "Rec Mono Semicasual",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.fontWeight": "500",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontLigatures": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.scrollBeyondLastColumn": 10,
  "editor.cursorSurroundingLines": 4,
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.tree.indent": 16,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.useSplitJSON": true,
  "workbench.panel.defaultLocation": "bottom",
  "workbench.editor.limit.enabled": true,
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.centerLayout": false,
  "zenMode.hideActivityBar": false,
  "zenMode.hideTabs": false,
  "zenMode.hideLineNumbers": false,
  "window.autoDetectColorScheme": true,
  "diffEditor.renderSideBySide": true,
  "diffEditor.renderIndicators": false,
  "html.format.enable": false,
  "search.quickOpen.history.filterSortOrder": "recency",
  "terminal.integrated.rightClickBehavior": "default", // On windows do not act like every windows terminal
  "cSpell.userWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "administratief",
    "bindgen",
    "Branca",
    "bson",
    "canbe",
    "compair",
    "consts",
    "cpufrequency",
    "cpuinfo",
    "data",
    "Datas",
    "db",
    "dir",
    "Equalf",
    "Errf",
    "etag",
    "gorm",
    "graphiql",
    "hasher",
    "Identication",
    "infof",
    "installable",
    "isbooked",
    "isinvoice",
    "laravel",
    "libc",
    "medewerker",
    "mjarkk",
    "Monokai",
    "Msgf",
    "Nilf",
    "numa",
    "OFTYPE",
    "orko",
    "Pbkdf",
    "physicalcpu",
    "plex",
    "rapp",
    "repr",
    "rtcv",
    "sindresorhus",
    "splitn",
    "stretchr",
    "strongness",
    "struct",
    "structs",
    "subdir",
    "subdirs",
    "sysctlbyname",
    "sysinfo",
    "testingdb",
    "type",
    "typeography",
    "upsert",
    "urlencoding",
    "valyala",
    "vite",
    "werk",
    "with",
    "XSRF",
    "Yubikey"
  ],
  "[go]": {
    "editor.autoClosingBrackets": "languageDefined"
  },
  "workbench.colorTheme": "Monokai Pro",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "rust": false
  },
  "editor.largeFileOptimizations": false,
  "telemetry.telemetryLevel": "off",
  "editor.lineNumbers": "off",
  "errorLens.enabledDiagnosticLevels": [
    "error",
    "warning"
  ],
  "breadcrumbs.enabled": false,
  "workbench.activityBar.visible": false
}

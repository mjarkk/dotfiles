{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.decorations.badges": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "languageStylus.useSeparator": false,
    "editor.fontSize": 16,
    "editor.fontFamily": "Fira Code", // "iosevka" is an alternative nice front
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.fontWeight": "500",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.scrollBeyondLastColumn": 10,
    "editor.cursorSurroundingLines": 4,
    "workbench.colorTheme": "Github Light Theme - Gray",
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "workbench.list.keyboardNavigation": "simple",
    "workbench.tree.indent": 16,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.panel.defaultLocation": "bottom",
    "workbench.editor.limit.enabled": true,
    "workbench.tips.enabled": false,
    "workbench.statusBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Spectrum)",
    "workbench.preferredLightColorTheme": "Github Light Theme - Gray",
    "zenMode.centerLayout": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideTabs": false,
    "zenMode.hideLineNumbers": false,
    "window.menuBarVisibility": "hidden",
    "window.autoDetectColorScheme": true,
    "window.zoomLevel": -1,
    "diffEditor.renderSideBySide": true,
    "diffEditor.renderIndicators": false,
    "html.format.enable": false,
    "search.quickOpen.history.filterSortOrder": "recency",
    "terminal.integrated.rightClickBehavior": "default", // On windows do not act like every windows terminal
    "cSpell.userWords": [
        "Branca",
        "Datas",
        "Identication",
        "Yubikey",
        "bson",
        "canbe",
        "compair",
        "consts",
        "cpufrequency",
        "cpuinfo",
        "dir",
        "etag",
        "graphiql",
        "hasher",
        "isbooked",
        "isinvoice",
        "laravel",
        "libc",
        "numa",
        "orko",
        "physicalcpu",
        "plex",
        "rapp",
        "sindresorhus",
        "splitn",
        "strongness",
        "structs",
        "subdir",
        "subdirs",
        "sysctlbyname",
        "sysinfo",
        "typeography",
        "upsert",
        "urlencoding",
        "vite",
        "with"
    ],
    "vetur.format.defaultFormatter.css": "none",
    "vetur.format.defaultFormatter.html": "none",
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "vetur.format.defaultFormatter.less": "none",
    "vetur.format.defaultFormatter.postcss": "none",
    "vetur.format.defaultFormatter.pug": "none",
    "vetur.format.defaultFormatter.sass": "none",
    "vetur.format.defaultFormatter.scss": "none",
    "vetur.format.defaultFormatter.stylus": "none",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "go.toolsManagement.autoUpdate": true,
}

{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.decorations.badges": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "languageStylus.useSeparator": false,
    "editor.fontSize": 16,
    "editor.fontFamily": "Rec Mono Semicasual", // "Fira Code" and "iosevka" are also nice fronts, this font: https://www.recursive.design
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.fontWeight": "500",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.scrollBeyondLastColumn": 10,
    "editor.cursorSurroundingLines": 4,
    "workbench.iconTheme": "Monokai Pro Monochrome Icons",
    "workbench.list.keyboardNavigation": "simple",
    "workbench.tree.indent": 16,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.panel.defaultLocation": "bottom",
    "workbench.editor.limit.enabled": true,
    "workbench.tips.enabled": false,
    "workbench.statusBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.preferredLightColorTheme": "Github Light Theme - Gray",
    "zenMode.centerLayout": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideTabs": false,
    "zenMode.hideLineNumbers": false,
    "window.menuBarVisibility": "hidden",
    "window.autoDetectColorScheme": true,
    "diffEditor.renderSideBySide": true,
    "diffEditor.renderIndicators": false,
    "html.format.enable": false,
    "search.quickOpen.history.filterSortOrder": "recency",
    "terminal.integrated.rightClickBehavior": "default", // On windows do not act like every windows terminal
    "cSpell.userWords": [
        "abcdefghijklmnopqrstuvwxyz",
        "administratief",
        "Branca",
        "bson",
        "canbe",
        "compair",
        "consts",
        "cpufrequency",
        "cpuinfo",
        "data",
        "Datas",
        "db",
        "dir",
        "Equalf",
        "Errf",
        "etag",
        "gorm",
        "graphiql",
        "hasher",
        "Identication",
        "infof",
        "installable",
        "isbooked",
        "isinvoice",
        "laravel",
        "libc",
        "medewerker",
        "mjarkk",
        "Monokai",
        "Msgf",
        "Nilf",
        "numa",
        "OFTYPE",
        "orko",
        "physicalcpu",
        "plex",
        "rapp",
        "rtcv",
        "sindresorhus",
        "splitn",
        "stretchr",
        "strongness",
        "struct",
        "structs",
        "subdir",
        "subdirs",
        "sysctlbyname",
        "sysinfo",
        "testingdb",
        "type",
        "typeography",
        "upsert",
        "urlencoding",
        "valyala",
        "vite",
        "werk",
        "with",
        "XSRF",
        "Yubikey"
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[go]": {
        "editor.autoClosingBrackets": "languageDefined",
    },
    "go.toolsManagement.autoUpdate": true,
    "workbench.preferredDarkColorTheme": "Monokai Pro",
    "workbench.colorTheme": "Github Light Theme - Gray",
    "redhat.telemetry.enabled": false,
    "window.zoomLevel": -1,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "rust": false
    },
    "dart.debugSdkLibraries": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.largeFileOptimizations": false,
    "go.lintTool": "golint",
    "telemetry.telemetryLevel": "off",
}

{
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 18,
    "editor.tabSize": 2,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.fontFamily": "Fira Code",
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "window.zoomLevel": 0,
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "workbench.colorTheme": "Dobri Next -C08- Darcula",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.iconTheme": "Monokai Pro Icons",
    "languageStylus.useSeparator": false,
    "search.exclude": {
        "**/vendor": true,
    },
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "workbench.sideBar.location": "right",
    "workbench.list.keyboardNavigation": "simple",
    "editor.fontWeight": "500",
    "cSpell.enabledLanguageIds": [
        "markdown"
    ],
    "cSpell.language": "nl,nl-NL,en",
    "editor.cursorBlinking": "smooth",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": true,
    "files.watcherExclude": {
        "**/vendor/**": true
    },
    "workbench.enableExperiments": false,
    "workbench.useExperimentalGridLayout": true,
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 16,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideTabs": false,
    "window.menuBarVisibility": "toggle",
    "window.restoreFullscreen": true,
    "window.title": "${dirty}${activeEditorShort} <> ${rootName}",
    "explorer.openEditors.visible": 0,
    "terminal.integrated.experimentalRefreshOnResume": true
}

